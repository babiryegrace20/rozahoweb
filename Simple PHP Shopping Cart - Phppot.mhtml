		</span><span class="tag">&lt;div</span><span class="pln"> </span><span class="atn">class</span><span class="pun">=</span><span class="atv">"product-image"</span><span class="tag">&gt;</span><span class="pln">&lt;img src="</span><span class="pun">&lt;?</span><span class="pln">php echo $product_array</span><span class="pun">[</span><span class="pln">$key</span><span class="pun">][</span><span class="str">"image"</span><span class="pun">];</span><span class="pln"> </span><span class="pun">?&gt;</span><span class="pln">"&gt;</span><span class="tag">&lt;/div&gt;</span><span class="pln">
