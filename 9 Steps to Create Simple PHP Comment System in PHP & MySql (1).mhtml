Content-Transfer-Encoding: binary
Content-Location: https://secure.gravatar.com/dist/css/hovercard.min.css?ver=201952

@charset "utf-8";

.gcard { position: absolute; z-index: 20; box-shadow: rgba(0, 0, 0, 0.4) 0px 0px 8px; max-width: 400px; margin: 0px; }

.gcard .grav-inner { zoom: 1; position: relative; background: rgba(0, 0, 0, 0.85); padding: 18px; color: rgb(255, 255, 255); font-family: Helvetica, Arial, Tahoma, sans-serif; font-size: 12px; line-height: 150%; text-shadow: rgb(0, 0, 0) 0px -1px 0px; }

.gcard .grav-inner, .gcard .grav-inner * { font-style: normal !important; }

.gcard .gcard-about { clear: left; }

.gcard a { font-family: Helvetica, Arial, Tahoma, sans-serif; color: rgb(255, 255, 255) !important; }

.gcard a, .gcard a:hover { border: none !important; text-decoration: none !important; }

.gcard a:hover { color: rgb(81, 166, 218) !important; }

.gcard ul { margin: 0px; padding: 0px; font-size: 12px; }

.gcard ul, .gcard ul li { list-style: none; }

.gcard p { color: rgb(255, 255, 255); text-align: left; font-size: 12px; margin: 0px 0px 10px; }

.gcard .grav-small { font-size: 10px; }

.gcard .grav-grav { float: left; border: 3px solid rgb(255, 255, 255); box-shadow: rgb(100, 168, 209) 0px 1px 0px inset, rgb(0, 0, 0) 0px 1px 5px; line-height: 50%; padding: 0px !important; }

.pos-left .grav-grav { float: right; }

.gcard .grav-grav img { padding: 0px; margin: 0px; }

.gcard .grav-info { float: left; width: 220px; margin-left: 20px; }

.pos-left .grav-info { float: right; margin-left: 0px; margin-right: 20px; }

.gcard h4 { float: left; font-size: 18px; font-family: Helvetica, Arial, Tahoma, sans-serif; line-height: 115%; vertical-align: middle; text-transform: none; text-align: left; margin: 0px 10px 5px 0px !important; padding: 0px !important; }

.gcard .grav-about, .gcard .grav-gallery, .gcard .grav-links, .gcard .grav-services { display: none; }

.gcard-about .grav-about, .gcard-gallery .grav-gallery, .gcard-links .grav-links, .gcard-services .grav-services { display: block; }

.grav-about { clear: left; }

a.grav-edit-profile { border-bottom: 1px solid rgb(102, 102, 102) !important; }

a.grav-edit-profile:hover { border-bottom: 1px solid rgb(68, 68, 68) !important; }

.grav-loc { clear: left; margin: 0px 0px 1px; font-size: 90% !important; color: rgb(159, 160, 159) !important; }

.gcard .grav-cardarrow { position: absolute; top: 23px; left: -8px; display: inline-block; border-top: 8px solid transparent; border-bottom: 8px solid transparent; border-right: 8px solid rgba(0, 0, 0, 0.85); width: 0px !important; height: 0px !important; }

.pos-left .grav-cardarrow { left: auto; border-left: 8px solid rgba(0, 0, 0, 0.85); border-right: none; right: -8px !important; }

.gcard .grav-cardarrow { display: none; }

.gcard .grav-tag { clear: both; position: absolute; top: 0px; right: -22px; background: url("/images/grav-tag.png") 100% 0px / 48px 61px no-repeat; width: 48px; height: 61px; }

.pos-left .grav-tag { right: auto; left: -22px; background: url("/images/grav-tag-left.png") 100% 0px / 48px 61px no-repeat; }

.gcard .grav-tag a { width: 48px; height: 61px; display: block; text-decoration: none; }

.pos-left .grav-tag a { transform: scaleX(-1); }

.gcard .grav-extra { float: left; margin: 5px 5px 0px 0px; vertical-align: middle; }

.gcard .grav-extra a { display: block; line-height: 80%; font-size: 11px; border-radius: 3px; text-shadow: none; padding: 3px 5px 3px 17px !important; color: rgb(0, 0, 0) !important; }

.gcard .grav-extra a:hover { background-color: rgb(59, 145, 197); color: rgb(255, 255, 255) !important; }

.gcard .comments a { background: url("/images/pillicon.png") 3px 2px no-repeat rgb(255, 255, 255); }

.gcard .comments a:hover { background-position: -11px 2px; }

.gcard .likes a { background: url("/images/pillicon.png") 4px -13px no-repeat rgb(255, 255, 255); }

.gcard .likes a:hover { background-position: -10px -13px; }

.gcard .grav-disable { margin: 3px 0px -5px; font-size: 10px; line-height: 10px; }

.gcard .grav-disable a { color: rgb(102, 102, 102) !important; }

.gcard .grav-disable a:hover { color: rgb(255, 255, 255) !important; }

a.grav-view-complete { display: inline-block; padding: 5px 15px; margin: 0px 0px 10px; background: -webkit-linear-gradient(top, rgb(61, 146, 198), rgb(14, 116, 178)); border-radius: 2px; border: none; box-shadow: rgb(100, 168, 209) 0px 1px 0px inset, rgb(0, 0, 0) 0px 1px 3px; color: rgb(255, 255, 255); text-shadow: rgb(18, 72, 105) 0px -1px 0px; font-size: 12px; font-family: "Helvetica Neue Bold", "Helvetica Neue", Helvetica, Arial, sans-serif; font-weight: 700; text-decoration: none; }

a.grav-view-complete:hover { background: -webkit-linear-gradient(top, rgb(81, 166, 218), rgb(61, 146, 198)); box-shadow: rgb(134, 193, 229) 0px 1px 0px inset, rgb(0, 0, 0) 0px 1px 5px; color: rgb(255, 255, 255) !important; }

a.grav-view-complete:active { background: -webkit-linear-gradient(top, rgb(14, 116, 178), rgb(61, 146, 198)); box-shadow: rgb(79, 156, 203) 0px -1px 0px inset, rgb(0, 0, 0) 0px 1px 1px; color: rgb(211, 230, 241); }

@media not all, only screen and (-webkit-min-device-pixel-ratio: 1.5), not all, not all {
  .gcard .grav-tag { background-image: url("/images/grav-tag-2x.png"); }
  .pos-left .grav-tag { background-image: url("/images/grav-tag-left-2x.png"); }
}
