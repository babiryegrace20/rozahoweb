@charset "utf-8";

body, html { margin: 0px; padding: 0px; overflow: visible; text-align: center; font-family: HelveticaNeue, Arial, sans-serif; background: transparent; }

.ajximg { left: -789em; }

.ajximg, .widget { position: absolute; }

.widget { top: 0px; left: 0px; text-align: center; white-space: nowrap; }

.widget.center { position: relative; margin: 0px auto; }

.widget.right { left: auto; right: 0px; width: auto !important; }

.widget.pocket, .widget.pocket.vertical, .widget.save, .widget.save.vertical { width: 60px; }

.widget.horizontal { width: 115px; }

#btn { display: block; cursor: pointer; }

#btn span { position: relative; display: block; border: 1px solid rgb(187, 187, 187); font-weight: 700; color: rgb(119, 119, 119); background: rgb(255, 255, 255); }

#btn:hover span { color: rgb(85, 85, 85); }

.horizontal #btn span { float: left; height: 18px; padding: 0px 6px; line-height: 18px; font-size: 11px; font-weight: 400; border-radius: 3px; }

.vertical #btn span { height: 33px; line-height: 33px; margin-bottom: 8px; font-size: 14px; }

#btn span em { font-style: normal; transition: opacity 0.3s ease 0s; }

#btn span em.fadeOut { opacity: 0; }

#btn span i, #btn span u { display: block; position: absolute; border: 4px solid transparent; width: 0px; height: 0px; line-height: 0; zoom: 1; z-index: 10; }

.vertical #btn span i, .vertical #btn span u { left: 50%; bottom: 0px; margin: 0px 0px -4px -4px; border-bottom: 0px; }

.vertical #btn span i { border-top-color: rgb(170, 170, 170); }

.vertical #btn span u { margin-bottom: -3px; border-top-color: rgb(255, 255, 255); }

.horizontal #btn span i, .horizontal #btn span u { top: 50%; left: 0px; margin: -4px 0px 0px -4px; border-left: 0px; }

.horizontal #btn span i { border-right-color: rgb(170, 170, 170); }

.horizontal #btn span u { margin-left: -3px; border-right-color: rgb(255, 255, 255); }

#btn b { display: block; height: 20px; line-height: 20px; font-size: 12px; background: url("//assets.getpocket.com/web/widgetButton/images/pocket_button.d3a57cce2ccd22d7db8e29ff66dc09df.png") center top / 60px no-repeat; }

#btn:hover b { background-position: 50% center; }

#btn.saved b { background-position: center bottom; }

.pocket #btn b { width: 60px; }

.horizontal.pocket #btn { padding-left: 66px; }

.save #btn b { width: 60px; }

.horizontal #btn b { position: absolute; top: 0px; left: 0px; }

.save #btn b { background-size: 60px; background-image: url("//assets.getpocket.com/web/widgetButton/images/save_button.1efc3627d1ec2de3e75f2a015a48f9f2.png"); }

@media only screen and (-webkit-min-device-pixel-ratio: 1.5), only screen and (min-resolution: 144dpi) {
  #btn b { background-image: url("//assets.getpocket.com/web/widgetButton/images/pocket_button-2x.2ef66f24d56b85fe970483f888e7067f.png"); }
  .save #btn b { background-image: url("//assets.getpocket.com/web/widgetButton/images/save_button-2x.f4f81b4076f38132a5453b6831c80d49.png"); }
}
