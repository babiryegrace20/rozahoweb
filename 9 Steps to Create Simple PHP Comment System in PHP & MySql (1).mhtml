ins { text-decoration: none; }

del { text-decoration: line-through; }

table { border-collapse: collapse; border-spacing: 0px; }

strong, b { font-weight: 500; }

body { border: 0px; }

a, abbr, acronym, address, applet, b, big, blockquote { margin: 0px; padding: 0px; border: 0px; font-size: 100%; background: transparent; }

button { margin: 0px; padding: 0px; border: 0px; background: transparent; }

canvas, caption, center, cite, code, dd, del, dfn, div, dl, dt, em, embed, fieldset, font, form { margin: 0px; padding: 0px; border: 0px; font-size: 100%; background: transparent; }

h1, h2, h3, h4, h5, h6 { margin: 0px; padding: 0px; border: 0px; background: transparent; }

hr, html, i, iframe, img, ins, kbd, label, legend, li, menu, object, ol, p, pre, q, s, samp, small, span, strike, strong, sub { margin: 0px; padding: 0px; border: 0px; font-size: 100%; background: transparent; }

sup { margin: 0px; padding: 0px; border: 0px; background: transparent; }

table, tbody, td, tfoot, th, thead, tr, tt, u, ul, var { margin: 0px; padding: 0px; border: 0px; font-size: 100%; background: transparent; }

body { overflow-wrap: break-word; font: 12px "YouTube Noto", Roboto, arial, sans-serif; }

button, input, textarea { font: 12px "YouTube Noto", Roboto, arial, sans-serif; }

a { color: rgb(22, 122, 198); text-decoration: none; }

a:hover { text-decoration: underline; }

a:focus { outline: rgb(102, 102, 102) dotted 1px; border: 0px; }

button:focus { outline: rgb(118, 118, 118) solid 1px; }

h1, h2 { font-weight: normal; }

h3, h4, h5, h6 { font-weight: 500; }

