.thumbnails-v .trc_rbox_header { line-height: 1.2em; position: relative; display: none; width: auto; margin: 0px; background: transparent; height: auto; box-sizing: initial; }

.thumbnails-v .trc_rbox_header_span .trc_header_right_column { display: none; background: transparent; height: auto; }

.thumbnails-v .trc_rbox_header .logoDiv { font-size: inherit; line-height: normal; }

.thumbnails-v .logoDiv a { font-size: 100%; }

.thumbnails-v .logoDiv a span { display: inline; color: rgb(0, 0, 0); font-weight: normal; font-size: 11px; }

.thumbnails-v .videoCube a { padding: 0px; }

.thumbnails-v .thumbBlock { margin: 0px; }

.thumbnails-v .video-label-box { margin: 5px 0px 0px; height: 88px; }

.thumbnails-v .syndicatedItem .video-label-box { margin: 5px 0px 0px; height: 88px; }

.thumbnails-v .videoCube .video-label-box .video-title { text-decoration: none; margin: 0px; }

.thumbnails-v .videoCube:hover .video-label-box .video-title, .thumbnails-v .videoCube:hover .video-label-box .video-description { text-decoration: underline; }

.thumbnails-v .video-label-box .branding { display: block; line-height: 22px; }

.thumbnails-v .syndicatedItem .branding { line-height: 22px; }

.thumbnails-v .trc_header_left_column { background: transparent; height: auto; }

.thumbnails-v .trc_header_right_part { margin-top: 0px; }

.thumbnails-v .trc_rbox_header .logoDiv a { font-size: 100%; }

.thumbnails-v .trc_rbox_header .trc_header_ext { position: relative; top: auto; right: auto; }
