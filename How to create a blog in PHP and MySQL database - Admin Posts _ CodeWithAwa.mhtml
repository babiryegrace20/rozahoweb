@charset "utf-8";

h1, h2, h3, h4, h5, h6 { font-family: Nunito, sans-serif; }

html { height: 100%; box-sizing: border-box; }

body { position: relative; margin: 0px; padding-bottom: 16rem; min-height: 100%; background: rgb(246, 246, 246); }

a:hover { text-decoration: none; }

.posts-wrapper { margin-top: 20px; border-radius: 3px; padding: 0px; font-family: Nunito, sans-serif; }

.posts-wrapper p { margin: 35px auto; }

.posts-wrapper .tut_post { background: white; max-height: 300px; overflow: hidden; border: 1px solid rgb(224, 227, 233); padding: 10px 10px 0px; margin-top: 10px; color: rgb(68, 68, 68); font-size: 1.1em; }

.posts-wrapper .tut_post a { color: rgb(68, 68, 68); text-decoration: none; }

.posts-wrapper .tut_post h3 { font-size: 1.35em; }

.tut_post_title { width: 70%; padding-bottom: 15px; float: left; }

.tut_post_title .post-title { margin: 5px 0px; color: rgb(0, 102, 102); }

.tut_post_image_div { width: 30%; max-width: 100%; max-height: 134px; float: left; }

.tut_post_image_div img { width: 90%; height: 50%; max-height: 100px; margin: 1.5em auto; }

.tut_post_body { font-size: 1.2em; font-family: Nunito, sans-serif; }

.tut_post_footer_div { background: white; padding: 0px 0px 10px; }

.posts-filters { margin-left: 20px; }

.posts-wrapper.details-page { background: white; font-family: Nunito, sans-serif; line-height: 1.6em; font-size: 1.3em; color: rgb(68, 68, 68); padding: 20px; text-align: justify; }

.newsletter { background: white; padding: 10px 20px; }

.newsletter h3 { margin-top: 0px; margin-bottom: 20px; text-align: center; font-family: Nunito, sans-serif; }

.newsletter_input { line-height: 1.5; margin: 5px auto; border-radius: 2px; padding-left: calc(0.625em - 1px); padding-right: calc(0.625em - 1px); width: 100%; height: 40px; max-width: 100%; border: 1px solid rgb(204, 204, 204); }

.newsletter_input:focus { outline: none; border: 1px solid rgb(0, 102, 102); }

.popular-posts-wrapper { background: white; margin-top: 20px; font-family: Nunito, sans-serif; line-height: 1.1em; font-size: 1.1em; color: rgb(68, 68, 68); padding: 10px; }

.popular-post:hover { cursor: pointer; text-decoration: underline; }

.popular-post .popular-post-title { color: rgb(68, 68, 68); border-bottom: 1px dashed rgb(204, 221, 221); margin: 10px 0px; }

.popular-post .popular-post-title:hover { color: rgb(0, 102, 102); }

.topics-wrapper { background: white; padding-top: 10px; margin-top: 20px; }

.section-header { color: rgb(16, 4, 87); }

.topics-wrapper .section-header { margin: 20px 0px 15px 20px; color: rgb(16, 4, 87); }

.topic { border-top: 1px dashed rgb(215, 215, 215); padding: 3px 0px; margin: 0px 0px 0px 5px; position: relative; }

.topic:hover { cursor: pointer; }

.right-chevron { color: rgb(119, 119, 119); position: absolute; top: 18px; left: 3px; font-size: 0.2em; }

.sign_up_btn.custom_sign_up_btn { background: rgb(61, 190, 139); border: 1px solid rgb(31, 124, 87); box-shadow: rgb(79, 211, 170) 0px 1px inset; color: rgb(255, 255, 255); position: relative; text-decoration: none; text-shadow: rgb(28, 143, 61) 0px 1px 0px; letter-spacing: 0.13em; transition: background 150ms ease 0s; font-size: 14px; padding: 6px 10px; text-transform: none; border-radius: 4px; }

.sign_up_btn:hover { cursor: pointer; }

.form-nav-search { padding: 0px; margin: 7px 0px 0px 100px; min-width: 30%; }

pre { padding: 0px; }

.media-platform:hover { cursor: pointer; }

.footer { position: absolute; right: 0px; bottom: 0px; left: 0px; color: white; background-color: rgb(41, 44, 47); text-align: center; margin: 320px auto 0px; padding: 20px 0px; }

.footer a { color: white; }

.footer p { margin: 0px; padding: 0px; }

.footer .copyright { color: rgb(135, 138, 142); }

.footer .footer-links { font-size: 1.2em; }

.footer .footer-links a:hover { color: gold; }

.square-ad { display: block; margin: 20px auto; width: 50%; }

.ad { margin: 15px auto; }

.marker { padding: 1px 2px; font-family: Consolas, Menlo, Monaco, "Lucida Console", "Liberation Mono", "DejaVu Sans Mono", "Bitstream Vera Sans Mono", "Courier New", monospace, sans-serif; background: rgb(230, 230, 230); border-radius: 2px; font-size: 0.91em; }
