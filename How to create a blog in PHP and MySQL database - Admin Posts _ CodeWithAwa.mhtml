@charset "utf-8";

.footer-distributed { background-color: rgb(41, 44, 47); box-shadow: rgba(0, 0, 0, 0.12) 0px 1px 1px 0px; box-sizing: border-box; width: 100%; font: bold 16px sans-serif; text-align: left; padding: 20px 60px 10px; margin-top: 80px; overflow: hidden; }

.footer-distributed .footer-left { float: left; }

.footer-distributed h3 { color: rgb(255, 255, 255); font: 36px Cookie, cursive; margin: 0px 0px 10px; }

.footer-distributed h3 span { color: rgb(83, 131, 211); }

.footer-distributed .footer-links { color: rgb(255, 255, 255); margin: 0px 0px 10px; padding: 0px; }

.footer-distributed .footer-links a { display: inline-block; line-height: 1.8; text-decoration: none; color: inherit; }

.footer-distributed .footer-links a:hover { color: gold; }

.footer-distributed .footer-company-name { color: rgb(143, 146, 150); font-size: 14px; font-weight: normal; margin: 0px; }

.footer-distributed .footer-icons { margin-top: 40px; }

.footer-distributed .footer-icons a { display: inline-block; width: 35px; height: 35px; cursor: pointer; background-color: rgb(51, 56, 59); border-radius: 2px; font-size: 20px; color: rgb(255, 255, 255); text-align: center; line-height: 35px; margin-right: 3px; margin-bottom: 5px; }

.footer-distributed .footer-right { float: right; }

.footer-distributed .footer-right p { display: inline-block; vertical-align: top; margin: 15px 42px 0px 0px; color: rgb(255, 255, 255); }

.footer-distributed form { display: inline-block; }

.footer-distributed form input, .footer-distributed form textarea { display: block; border-radius: 3px; box-sizing: border-box; background-color: rgb(31, 32, 34); box-shadow: rgba(255, 255, 255, 0.1) 0px 1px 0px 0px; border: none; resize: none; font-style: inherit; font-variant: inherit; font-stretch: inherit; line-height: inherit; font-family: inherit; font-size: 14px; font-weight: normal; color: rgb(209, 210, 210); width: 400px; padding: 18px; }

.footer-distributed ::-webkit-input-placeholder { color: rgb(92, 102, 107); }

.footer-distributed form input { height: 55px; margin-bottom: 15px; }

.footer-distributed form textarea { height: 100px; margin-bottom: 20px; }

.footer-distributed form button { border-radius: 3px; background-color: rgb(51, 56, 59); color: rgb(255, 255, 255); border: 0px; padding: 15px 50px; font-weight: bold; float: right; }

@media (max-width: 1000px) {
  .footer-distributed { font: bold 14px sans-serif; }
  .footer-distributed .footer-company-name { font-size: 12px; }
  .footer-distributed form input, .footer-distributed form textarea { width: 250px; }
  .footer-distributed form button { padding: 10px 35px; }
}

@media (max-width: 800px) {
  .footer-distributed { padding: 30px; }
  .footer-distributed .footer-left, .footer-distributed .footer-right { float: none; max-width: 300px; margin: 0px auto; }
  .footer-distributed .footer-left { margin-bottom: 40px; }
  .footer-distributed form { margin-top: 30px; }
  .footer-distributed form { display: block; }
  .footer-distributed form button { float: none; }
}
