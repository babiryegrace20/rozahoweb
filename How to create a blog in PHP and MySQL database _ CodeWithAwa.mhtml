	<span class="token markup"><span class="token comment" spellcheck="true">&lt;!-- Styling <span class="hljs-keyword">for</span> <span class="hljs-keyword">public</span> area --&gt;</span></span>
	<span class="token markup"><span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span>link</span> <span class="token attr-name">rel</span><span class="token attr-value"><span class="token punctuation">=</span><span class="token punctuation"><span class="hljs-string">"</span></span><span class="hljs-string">stylesheet</span><span class="token punctuation"><span class="hljs-string">"</span></span></span> <span class="token attr-name">href</span><span class="token attr-value"><span class="token punctuation">=</span><span class="token punctuation"><span class="hljs-string">"</span></span><span class="hljs-string">static/css/public_styling.css</span><span class="token punctuation"><span class="hljs-string">"</span></span></span><span class="token punctuation">&gt;</span></span></span>
