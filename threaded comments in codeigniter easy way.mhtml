.tag_nav li a:hover { background: rgb(25, 189, 155); color: rgb(255, 255, 255); }

.news_letter { margin: 8% 0px; }

.news_letter form input[type="text"] { font-family: "Open Sans", sans-serif; font-weight: 100; padding: 10px; display: block; width: 93%; background: rgb(255, 255, 255); outline: none; color: rgb(85, 85, 85); font-size: 0.8725em; border: 1px solid rgb(218, 218, 218); -webkit-appearance: none; }

.news_letter form input[type="submit"] { margin-top: 4%; -webkit-appearance: none; float: right; font-family: "Open Sans", sans-serif; cursor: pointer; border: none; outline: none; display: inline-block; padding: 12px 30px; font-size: 0.8925em; color: rgb(255, 255, 255); background: rgb(25, 189, 155); text-transform: uppercase; border-radius: 2px; transition: all 0.3s ease-in-out 0s; }

.news_letter form input[type="submit"]:hover { background: rgb(52, 73, 94); }

.sap_tabs { clear: both; }

.facts { padding: 10px 0px; }

.tab-left { margin: 4% 0px; }

.top1 { margin-top: 2%; }

.resp-tabs-list { width: 100%; }

.resp-tab-item:first-child { border-left: none; }

.resp-tab-item { border-left: 1px solid rgb(192, 191, 191); border-bottom: 3px solid rgb(25, 189, 155); font-weight: 600; color: rgb(52, 73, 94); font-size: 1em; background: rgb(231, 231, 231); text-transform: capitalize; cursor: pointer; padding: 12px 0px; display: inline-block; text-align: center; list-style: none; float: left; width: 33%; outline: none; transition: all 0.3s ease-out 0s; }

.resp-tab-item:hover { background: rgb(52, 73, 94); text-shadow: none; color: rgb(255, 255, 255); }

.resp-tab-active { background: rgb(25, 189, 155); text-shadow: none; color: rgb(255, 255, 255); }

.resp-tabs-container { padding: 0px; background-color: rgb(255, 255, 255); clear: left; }

h2.resp-accordion { cursor: pointer; padding: 5px; display: none; }

.resp-tab-content { display: none; }

.resp-content-active, .resp-accordion-active { display: block; }

h2.resp-accordion { font-size: 1em; padding: 10px 15px; background: rgb(197, 205, 209); margin: 10px 0px; color: rgb(255, 255, 255); }

h2.resp-accordion:hover { background: rgb(254, 34, 50); text-shadow: none; color: rgb(255, 255, 255); }

span.tab-img { float: left; width: 26.3333%; margin-right: 6.3333%; }

.tab-text { float: left; width: 66.3333%; }

.tab-text p { margin-bottom: 10px; }

.tab-text p a { font-weight: 600; color: rgb(52, 73, 94); font-size: 0.8925em; line-height: 1.5em; text-transform: capitalize; }

.tab-text p a:hover { color: rgb(25, 189, 155); }

.post-meta { font-size: 14px; margin-bottom: 10px; }

.post-meta img { vertical-align: middle; margin-top: -2px; margin-right: 10px; }

.post-meta a { color: rgb(141, 148, 158); font-size: 0.8725em; text-transform: capitalize; }

.post-meta a:hover { color: rgb(25, 189, 155); }

.social_network_likes { margin: 0em 0px 1em; text-align: center; }

.social_network_likes li { float: left; width: 25%; }

.social_network_likes li a { display: block; color: rgb(255, 255, 255); transition: all 0.3s ease-in-out 0s; }

.social_network_likes li a p { padding: 15px 0px; font-size: 0.8em; }

.social_network_likes li a span { display: block; font-size: 2em; }

.social_network_likes li a { border-left: 2px solid rgb(255, 255, 255); }

.social_network_likes li a div.followers { border-bottom: 2px solid rgb(255, 255, 255); }

.social_network_likes li:first-child a { border-left: none; }

.social_network_likes li a.tweets { background: rgb(45, 165, 218); }

.social_network_likes li a.tweets:hover { background: rgb(26, 145, 197); }

.social_network_likes li a.facebook-followers { background: rgb(182, 199, 16); }

.social_network_likes li a.facebook-followers:hover { background: rgb(158, 173, 11); }

.social_network_likes li a.email { background: rgb(39, 193, 185); }

.social_network_likes li a.email:hover { background: rgb(11, 177, 168); }

.social_network_likes li a.dribble { background: rgb(207, 37, 85); }

.social_network_likes li a.dribble:hover { background: rgb(179, 16, 62); }

.social_network_likes li a div.social_network { padding: 10px 0px; }

.social_network i { width: 45px; height: 45px; display: block; background: url("../images/social-icons.png") no-repeat; margin: 0px auto; }

.social_network i.twitter-icon { background-position: 3px 2px; }

.social_network i.facebook-icon { background-position: -46px 3px; }

.social_network i.email-icon { background: url("../images/gplus.png") 4px 2px no-repeat scroll; }

.social_network i.dribble-icon { background: url("../images/youtube.png") 0px 0px no-repeat scroll; }

.contact_left { float: left; margin-right: 3.3333%; width: 32.3333%; }

.contact_right { float: left; width: 63.3333%; }

.contact_left h3 { margin-bottom: 2%; font-size: 1.2em; text-transform: capitalize; color: rgb(52, 73, 94); font-weight: 600; text-shadow: rgb(255, 255, 255) 0px 1px 0px; }

.company_address { padding-top: 10px; }

.company_address p { font-weight: 400; color: rgb(192, 192, 192); text-shadow: rgb(255, 255, 255) 0px 1px 0px; line-height: 1.8em; font-size: 0.8725em; }

.company_address p a { color: rgb(25, 189, 155); cursor: pointer; transition: all 0.3s ease-in-out 0s; }

.company_address p a:hover { color: rgb(192, 192, 192); }

.map { margin: 6% 0px 4%; }

.contact_right h3 { margin-bottom: 2%; font-size: 1.2em; text-transform: capitalize; color: rgb(52, 73, 94); font-weight: 600; text-shadow: rgb(255, 255, 255) 0px 1px 0px; }

.contact-form { position: relative; }

.contact-form div { padding: 5px 0px; }

.contact-form span { text-transform: capitalize; display: block; color: rgb(192, 192, 192); text-shadow: rgb(255, 255, 255) 0px 1px 0px; line-height: 1.5em; font-size: 0.8725em; font-weight: 400; }

span.incorrect { text-transform: capitalize; display: block; color: red; text-shadow: rgb(255, 255, 255) 0px 1px 0px; line-height: 1.5em; font-size: 0.8725em; font-weight: 400; }

.contact-form input[type="text"], .contact-form textarea { font-family: "Open Sans", sans-serif; padding: 10px; display: block; width: 96.3333%; background: rgb(255, 255, 255); outline: none; color: rgb(192, 192, 192); font-size: 0.8725em; border: 1px solid rgb(236, 236, 236); -webkit-appearance: none; }

.contact-form textarea { resize: none; height: 120px; }

.contact-form input[type="submit"] { -webkit-appearance: none; font-weight: 600; float: right; font-family: "Open Sans", sans-serif; cursor: pointer; border: none; outline: none; display: inline-block; padding: 12px 30px; font-size: 0.8925em; color: rgb(255, 255, 255); background: rgb(25, 189, 155); text-transform: uppercase; border-radius: 2px; transition: all 0.3s ease-in-out 0s; }

.contact-form input[type="submit"]:hover { background: rgb(52, 73, 94); }

.details h2 { font-size: 1.4em; color: rgb(52, 73, 94); font-weight: 400; }

.details img { margin: 2% 0px; padding: 5px; box-shadow: rgb(221, 221, 221) 0px 0px 5px; }

.notice { display: block; margin: 10px 0px; padding: 0px 10px; border-width: 1px 0px 0px; background: url("../images/h-grds.png") 0px -210px; vertical-align: middle; }

.notice b { display: inline-block; font: italic 20pt "simplified arabic"; vertical-align: middle; width: 100px; padding: 20px 70px 20px 0px; }

.error { color: rgb(199, 57, 11); background-color: rgb(255, 198, 202); }

.error b { color: rgb(233, 125, 41); background: url("../images/icons/error.png") 99% 50% no-repeat; }

.information { color: rgb(20, 108, 183); background-color: rgb(199, 226, 255); }

.information b { color: rgb(118, 155, 187); background: url("../images/icons/information.png") 99% 50% no-repeat; }

.attention { color: rgb(191, 104, 22); background-color: rgb(255, 227, 150); }

.attention b { color: rgb(245, 150, 18); background: url("../images/icons/attention.png") 99% 50% no-repeat; }

.succeed { color: white; background-color: rgb(25, 189, 155); }

.succeed b { color: rgb(121, 162, 63); background: url("../images/icons/succeed.png") 99% 50% no-repeat; }

.tip { color: rgb(137, 54, 165); background-color: rgb(234, 168, 255); }

.tip b { color: rgb(186, 110, 211); background: url("../images/icons/tip.png") 99% 50% no-repeat; }

.form .notice { background-position: 0px -210px; height: auto; margin: 10px; padding: 5px 10px; }

.form .notice b { font: bold 10pt "simplified arabic"; width: auto; padding: 0px 30px 0px 10px; }

.form .error { border: 1px solid rgb(247, 187, 190); }

.form .error b { background-image: url("../images/error.png"); }

.form .information { border: 1px solid rgb(193, 219, 244); }

.form .information b { background-image: url("../images/information.png"); }

.form .attention { border: 1px solid rgb(244, 221, 159); }

.form .attention b { background-image: url("../images/attention.png"); }

.form .succeed { border: 1px solid rgb(204, 244, 178); }

.form .succeed b { background-image: url("../images/succeed.png"); }

.form .tip { border: 1px solid rgb(219, 150, 242); }

.form .tip b { background-image: url("../images/tip.png"); }

.form a.close { margin: 2px 0px 0px; }

