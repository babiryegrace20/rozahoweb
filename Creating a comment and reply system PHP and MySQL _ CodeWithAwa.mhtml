					&lt;div class='comment-details'&gt;
						&lt;span class='comment-name'&gt;"</span> . getUsernameById($inserted_reply[<span class="hljs-string">'user_id'</span>]) . <span class="hljs-string">"&lt;/span&gt;
						&lt;span class='comment-date'&gt;"</span> . date(<span class="hljs-string">'F j, Y '</span>, strtotime($inserted_reply[<span class="hljs-string">'created_at'</span>])) . <span class="hljs-string">"&lt;/span&gt;
