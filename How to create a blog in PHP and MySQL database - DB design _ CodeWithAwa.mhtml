@charset "utf-8";

#panel_set > .panel { position: fixed; top: -100%; left: -100%; z-index: 2147483646; display: block; width: 100%; height: 100%; }

#panel_set img { border: 0px; }

#panel_set > .panel.visible, #panel_set .visible .panel { position: fixed; left: 0px; top: 0px; opacity: 1; }

#panel_set .panel-container { display: table; width: 100%; height: 100%; margin: 0px; padding: 0px; background-color: rgb(250, 250, 250); }

#panel_set .panel-row { display: block; padding: 0px 0px 0.3em; }

#panel_set .panel-row:first-child { padding: 0.3em; }

#panel_set .panel-content { vertical-align: top; display: table-cell; color: rgba(0, 0, 0, 0.4); transition: opacity 150ms linear 0s; text-align: center; font-family: Arial, sans-serif; }

.collapsed #panel_set .fade { opacity: 0.4; }

.jake-middle.survey-horiz #panel_set .panel-content { vertical-align: middle; }

#ad_closed_panel .btn { display: inline-block; border-radius: 2px; box-sizing: border-box; width: auto; text-decoration: none; white-space: nowrap; line-height: 1em; cursor: pointer; box-shadow: rgba(0, 0, 0, 0.12) 0px 0px 2px, rgba(0, 0, 0, 0.26) 0px 1px 3px !important; }

#closed_text, #final_closed_text { display: inline-block; line-height: 1.28em; font-size: 1.2em; }

#native_closed_text { display: flex; flex-direction: row; align-items: center; }

#closed_text img, #native_closed_text img, #final_closed_text img { margin: 0px 0px -0.34em; height: 1.25em; display: inline-block; width: auto; min-width: 3.75em; opacity: 0.4; }

#native_closed_text img { margin: 0px 0.3em -0.2em; height: 1.4em; }

.spacer { display: inline-block; padding: 0px; box-shadow: none; width: 3px; }

#ad_closed_panel .responsive-btn { max-width: 288px; width: 75%; white-space: normal; margin-bottom: 0.4em; }

.spacer { display: none; }

#panel_set .panel-content { vertical-align: top; }

.btn > span { display: inline-block; padding: 0.5em 0.6em; line-height: 1em; }

#ad_closed_panel .settings-btn { background-color: rgb(255, 255, 255); color: rgb(158, 158, 166); }

#ad_closed_panel .settings-btn:hover, #ad_closed_panel .settings-btn:active { background-color: rgb(245, 245, 245); }

.settings-text { white-space: nowrap; }

#report_btn.primary-btn { background-color: rgb(66, 133, 245); color: white; }

#report_btn.primary-btn:hover, #report_btn.primary-btn:active { background-color: rgb(50, 117, 229); }

.btn-icon { position: relative; display: inline-block; margin-bottom: -0.15em; height: 1em; width: 1em; opacity: 0.4; }

[dir="rtl"] .btn-icon { float: left; }

#confirmation_msg { font-weight: bold; color: rgb(51, 51, 51); }

#confirmation_msg, #final_closed_text { line-height: 1.1em; padding: 0.3em; }

#confirmation_msg, #final_closed_text { padding: 1em 0.3em; }

#post_survey_panel span { display: block; text-align: center; line-height: 1em; }

#post_survey_panel a, #post_survey_panel a:active, #post_survey_panel a:hover { text-decoration: none; color: rgb(66, 133, 245); }

#final_closed_panel, #final_closed_panel #final_closed_text { opacity: 0; transition: opacity 400ms linear 0s; }

#final_closed_panel span { white-space: normal; }

#final_closed_panel #final_closed_text.visible { opacity: 1; }

.fallback-wrap .btn { margin: 1px auto; white-space: normal; }

.close { position: absolute; top: 2px; right: 2px; color: black; font-size: 15px; line-height: 15px; opacity: 0.5; height: 15px; width: 15px; user-select: none; cursor: pointer; }

[dir="rtl"] .close { left: 2px; }
